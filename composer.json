{
    "name": "thelogicstudio/codat-php",
    "description": "Unofficial PHP SDK for Codat's API",
    "keywords": [
        "codat",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://logicstudio.nz",
    "license": "mit",
    "authors": [
        {
            "name": "The Logic Studio",
            "homepage": "https://logicstudio.nz",
            "email": "hello@logicstudio.nz"
        },
        {
            "name": "Ian Simpson",
            "email": "ian@logicstudio.nz"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "TheLogicStudio\\CodatPHP\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "TheLogicStudio\\CodatPHP\\Test\\" : "test/" }
    }
}
